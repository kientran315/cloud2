<h1>FPT greenwich hanoi</h1>
<a href="/">Homepage</a>
<h3>We are living in {{ year }}</h3>
<table border ="1">
    <tr>
        <th>Country</th>
        <th>Capital</th>
    </tr>
    {{#each capitals }}
        <tr>
            <td>{{ this.[0] }}</td><!--0:column 1 (country)-->
            <td>{{ this.[1] }}</td><!--1:column 1(capital)-->
        </tr>
    {{/each}}
</table>
<ul>
    {{#each clubs }}
         <li> {{ this }}</li>
    {{/each}}
   
</ul>
<table border="2">
    {{#each image }}
        <tr>
            <th>
                <img src="/images/{{ this }}" width="100",height="100">
            </th>
        </tr>
    {{/each}}
</table>